        "class DiffuserLoss(torch.nn.Module):\n",
        "    def __init__(self):\n",
        "        super(DiffuserLoss, self).__init__()\n",
        "\n",
        "    def forward(self, z, z_hat, predicted_latent, sigma, sigma_data = 0.5):\n",
        "\n",
        "        c_out = sigma*sigma_data/np.sqrt(sigma**2+sigma_data**2)\n",
        "        c_in = 1 / np.sqrt(sigma**2+sigma_data**2)\n",
        "        c_skip = sigma_data**2 / (sigma**2+sigma_data**2)\n",
        "        lamda = (sigma**2+sigma_data**2) / (sigma*sigma_data)**2\n",
        "\n",
        "        loss = lamda* c_out**2 * torch.sum( ( predicted_latent - (z_hat - c_skip*z)/c_out ) ** 2)\n",
        "\n",
        "        return loss\n",
        "\n",
