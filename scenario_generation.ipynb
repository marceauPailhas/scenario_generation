        "import numpy as np\n",
        "\n",
        "def pad_vectors(actor_trajectory, future_trajectory, target_length):\n",
        "    \"\"\"\n",
        "    Pads actor_trajectory[:-1, :] and future_trajectory to have two elements.\n",
        "    The first is left-padded, and the second is right-padded.\n",
        "\n",
        "    Parameters:\n",
        "        actor_trajectory (np.ndarray): Array of shape (N, M) for the actor's past trajectory.\n",
        "        future_trajectory (np.ndarray): Array of shape (K, M) for the actor's future trajectory.\n",
        "        target_length (int): Target number of rows for each padded trajectory.\n",
        "\n",
        "    Returns:\n",
        "        tuple: (left_padded, right_padded) padded arrays of shape (target_length, M).\n",
        "    \"\"\"\n",
        "    # Get the slicing and padding values\n",
        "    past_trajectory = actor_trajectory\n",
        "    pad_value_past = past_trajectory[0] if len(past_trajectory) > 0 else 0  # Use first row or 0\n",
        "    pad_value_future = future_trajectory[-1] if len(future_trajectory) > 0 else 0  # Use last row or 0\n",
        "\n",
        "    # Left-pad the past trajectory\n",
        "    left_padded = np.pad(\n",
        "        past_trajectory,\n",
        "        pad_width=((target_length - len(past_trajectory), 0), (0, 0)),  # Left pad rows\n",
        "        mode='constant',\n",
        "        constant_values=pad_value_past\n",
        "    )\n",
        "\n",
        "    # Right-pad the future trajectory\n",
        "    right_padded = np.pad(\n",
        "        future_trajectory,\n",
        "        pad_width=((0, target_length - len(future_trajectory)), (0, 0)),  # Right pad rows\n",
        "        mode='constant',\n",
        "        constant_values=pad_value_future\n",
        "    )\n",
        "\n",
        "    return left_padded, right_padded\n",
        "\n",
        "# # Example usage\n",
        "# actor_trajectory = np.array([[1, 2], [3, 4], [5, 6]])  # Example actor trajectory (N=3, M=2)\n",
        "# future_trajectory = np.array([[7, 8], [9, 10]])  # Example future trajectory (K=2, M=2)\n",
        "\n",
        "# target_length = 5\n",
        "# left_padded, right_padded = pad_vectors(actor_trajectory, future_trajectory, target_length)\n",
        "\n",
        "# print(\"Left-padded trajectory:\\n\", left_padded)\n",
        "# print(\"Right-padded trajectory:\\n\", right_padded)\n"
      ]
