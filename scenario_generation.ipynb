        "def plot_actor_bounding_box(\n",
        "    ax: plt.Axes, cur_location: NDArrayFloat, heading: float, color: str, bbox_size: Tuple[float, float],\n",
        "    image_size: Tuple[int, int], resolution: float , focal_position: Tuple[float, float]\n",
        ") -> None:\n",
        "    \"\"\"Plot an actor bounding box centered on the actor's current location.\n",
        "\n",
        "    Args:\n",
        "        ax: Axes on which actor bounding box should be plotted.\n",
        "        cur_location: Current location of the actor (2,).\n",
        "        heading: Current heading of the actor (in radians).\n",
        "        color: Desired color for the bounding box.\n",
        "        bbox_size: Desired size for the bounding box (length, width).\n",
        "    \"\"\"\n",
        "    (bbox_length, bbox_width) = bbox_size\n",
        "\n",
        "    # Compute coordinate for pivot point of bounding box\n",
        "    d = np.hypot(bbox_length, bbox_width)\n",
        "    theta_2 = math.atan2(bbox_width, bbox_length)\n",
        "    pivot_x = cur_location[0] - (d / 2) * math.cos(heading + theta_2)\n",
        "    pivot_y = cur_location[1] - (d / 2) * math.sin(heading + theta_2)\n",
        "\n",
        "    vehicle_bounding_box = Rectangle(\n",
        "        (pivot_x, pivot_y), bbox_length, bbox_width, angle = np.degrees(heading), color=color, zorder=_BOUNDING_BOX_ZORDER\n",
        "    )\n",
        "    ax.add_patch(vehicle_bounding_box)\n",
        "\n",
        "    # Add arrow for heading direction\n",
        "    arrow_length = bbox_length * 0.6  # Arrow length relative to bounding box size\n",
        "    arrow_dx = arrow_length * math.cos(heading)\n",
        "    arrow_dy = arrow_length * math.sin(heading)\n",
        "\n",
        "    # Starting point of the arrow (center of the bounding box)\n",
        "    arrow_start_x = cur_location[0]\n",
        "    arrow_start_y = cur_location[1]\n",
        "\n",
        "    ax.add_patch(FancyArrow(\n",
        "        arrow_start_x, arrow_start_y, arrow_dx, arrow_dy,\n",
        "        width=0.2, length_includes_head=True, head_width=0.4, color=\"red\", zorder=101\n",
        "    ))\n",
        "\n",
        "    # Generate prameters for a binary mask\n",
        "    mask_width,mask_height  = image_size\n",
        "    vertices = vehicle_bounding_box.get_corners()\n",
        "    x_min, x_max = np.min(vertices[:, 0]), np.max(vertices[:, 0])\n",
        "    y_min, y_max = np.min(vertices[:, 1]), np.max(vertices[:, 1])\n",
        "\n",
        "    # Create a grid of pixel coordinates\n",
        "    x_coords, y_coords = np.meshgrid(\n",
        "        np.linspace(0, 1, int(mask_width* resolution))*2*_PLOT_BOUNDS_BUFFER_M + focal_position[0]- _PLOT_BOUNDS_BUFFER_M ,\n",
        "        np.linspace(0, 1, int(mask_height* resolution))*2*_PLOT_BOUNDS_BUFFER_M + focal_position[1]- _PLOT_BOUNDS_BUFFER_M,\n",
        "        indexing=\"ij\",\n",
        "    )\n",
        "\n",
        "    # Flatten grid to test all points against rectangle path\n",
        "    points = np.vstack((x_coords.ravel(), y_coords.ravel())).T\n",
        "\n",
        "    # Update mask with rectangle pixels\n",
        "    # Filter points within the bounding box\n",
        "    in_bbox = (points[:, 0] >= x_min) & (points[:, 0] <= x_max) & (points[:, 1] >= y_min) & (points[:, 1] <= y_max)\n",
        "    filtered_points = points[in_bbox]\n",
        "\n",
        "    # Check if filtered points are inside the rectangle\n",
        "    result = np.array([is_point_in_rectangle(px, py, vertices) for px, py in filtered_points])\n",
        "\n",
        "    # Create a full boolean result array\n",
        "    mask = np.zeros((int(mask_width*resolution), int(mask_height*resolution)), dtype=np.float64)\n",
        "\n",
        "    mask = np.zeros(points.shape[0], dtype=bool)\n",
        "    mask[in_bbox] = result\n",
        "\n",
        "    return mask.reshape(int(mask_width* resolution),int(mask_height* resolution))"
      ]
    },
