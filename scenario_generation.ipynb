        "                    )\n",
        "                    for sample_index in range(sparse.shape[0])\n",
        "            ]\n",
        "            # Calculate the mean loss without detaching from the graph\n",
        "            loss = torch.mean(torch.stack(losses))  #+ 0.1 * criterion2(z_prob,interval_prob)\n",
        "\n",
        "\n",
        "            # Backward pass\n",
