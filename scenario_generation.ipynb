        "import torch.distributions as dist\n",
        "\n",
        "\n",
        "def get_interval_probabilities(z):\n",
        "      # Step 1: Flatten the tensor along dimensions (C, W, H)\n",
        "      B, C, W, H = z.size()\n",
        "      z_flattened = z.view(B, -1)  # Shape: (B, C*W*H)\n",
        "\n",
        "      # Step 2: Sort along the new second axis\n",
        "      z_sorted, _ = torch.sort(z_flattened, dim=1)  # Shape: (B, C*W*H)\n",
        "      #print(z_sorted)\n",
        "      # Step 3: Compute midpoints between consecutive elements\n",
        "      # Shift the sorted tensor left and right to compute midpoints\n",
        "      z_midpoints = (z_sorted[:, :-1] + z_sorted[:, 1:]) / 2  # Shape: (B, C*W*H - 1)\n",
        "      z_midpoints = torch.clamp(z_midpoints, -1e3, 1e3)\n",
        "      # Step 4: Create a standard normal distribution\n",
        "      normal_dist = dist.Normal(0, 1)\n",
        "\n",
        "      # Compute the cumulative probability (CDF) for each midpoint\n",
        "      cdf_midpoints = normal_dist.cdf(z_midpoints)  # Shape: (B, C*W*H - 1)\n",
        "\n",
        "      # Compute probabilities for each interval\n",
        "      # Pad cdf_midpoints with 0 (for -inf) at the start and 1 (for +inf) at the end\n",
        "      cdf_padded = torch.cat([torch.zeros(B, 1, device=z.device), cdf_midpoints, torch.ones(B, 1, device=z.device)], dim=1)\n",
        "\n",
        "      # Calculate interval probabilities by taking differences between consecutive CDF values\n",
        "      prob_vector = cdf_padded[:, 1:] - cdf_padded[:, :-1]  # Shape: (B, C*W*H)\n",
        "      return prob_vector\n"
      ]
