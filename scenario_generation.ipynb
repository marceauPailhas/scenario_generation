        "class Decoder(torch.nn.Module):\n",
        "  def __init__(self, output_channel):\n",
        "    super(Decoder, self).__init__()\n",
        "    self.relu = torch.nn.ReLU(inplace=True)\n",
        "    self.output_channel = output_channel\n",
        "\n",
        "    stride = 1\n",
        "    in_channels = 128\n",
        "    out_channels = 128\n",
        "\n",
        "    self.conv_res1_64 = torch.nn.Conv2d(in_channels, out_channels, kernel_size=3, stride=stride, padding=1, bias=True)\n",
        "    self.bn1_64 = torch.nn.BatchNorm2d(out_channels)\n",
        "    self.conv_res2_64 = torch.nn.Conv2d(out_channels, out_channels, kernel_size=3, stride=1, padding=1, bias=True)\n",
        "    self.bn2_64 = torch.nn.BatchNorm2d(out_channels)\n",
        "\n",
        "    self.conv_res1_32 = torch.nn.Conv2d(in_channels*2, out_channels, kernel_size=3, stride=stride, padding=1, bias=True)#*2 because of the concatenation of m and Epsilon(x)\n",
        "    self.bn1_32 = torch.nn.BatchNorm2d(out_channels)\n",
        "    self.conv_res2_32 = torch.nn.Conv2d(out_channels, out_channels, kernel_size=3, stride=1, padding=1, bias=True)\n",
        "    self.bn2_32 = torch.nn.BatchNorm2d(out_channels)\n",
        "\n",
        "    self.upsample = torch.nn.Upsample(scale_factor=2, mode='bilinear', align_corners=True)\n",
        "    self.projection_shortcut = nn.Conv2d(in_channels*2, out_channels, kernel_size=1)\n",
        "\n",
        "    self.conv_expansion_output = torch.nn.Conv2d(in_channels=out_channels, out_channels=output_channel, kernel_size=1, stride=1, padding=0)\n",
        "\n",
        "\n",
        "  def forward(self , x):\n",
        "    #32 pixels\n",
        "    out32 = self.relu(self.bn1_32(self.conv_res1_32(x)))\n",
        "    out32 = self.bn2_32(self.conv_res2_32(out32))\n",
        "    out32 += self.projection_shortcut(x)\n",
        "    out32 = self.relu(out32)\n",
        "\n",
        "    out32 = self.upsample(out32)\n",
        "    #64 pixels\n",
        "    out64 = self.relu(self.bn1_64(self.conv_res1_64(out32)))\n",
        "    out64 = self.bn2_64(self.conv_res2_64(out64))\n",
        "    out64 += out32\n",
        "    out64 = self.relu(out64)\n",
        "\n",
        "\n",
        "    out64 = self.conv_expansion_output(out64)\n",
        "    return out64"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 36,
