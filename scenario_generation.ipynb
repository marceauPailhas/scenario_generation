        "import torch\n",
        "import torch.nn as nn\n",
        "\n",
        "class SequenceEncoder(nn.Module):\n",
        "    def __init__(self, input_dim, d_model, n_heads, num_layers, dim_feedforward, output_dim):\n",
        "        \"\"\"\n",
        "        Args:\n",
        "            input_dim: Dimension of each numerical input feature.\n",
        "            d_model: Dimension of the internal feature representation.\n",
        "            n_heads: Number of attention heads.\n",
        "            num_layers: Number of transformer layers.\n",
        "            dim_feedforward: Dimension of feedforward layers.\n",
        "            output_dim: Size of the output encoding vector.\n",
        "        \"\"\"\n",
        "        super(SequenceEncoder, self).__init__()\n",
        "\n",
        "        # Linear layer to project input features to d_model\n",
        "        self.input_projection = nn.Linear(input_dim, d_model)\n",
        "\n",
        "        # Transformer encoder layers\n",
        "        self.encoder = nn.ModuleList([\n",
        "            TransformerLayer(d_model, n_heads, dim_feedforward)\n",
        "            for _ in range(num_layers)\n",
        "        ])\n",
        "\n",
        "        # Pooling and final projection to output_dim\n",
        "        self.pooling = nn.AdaptiveAvgPool1d(1)  # To reduce sequence to a single vector\n",
        "        self.fc_out = nn.Linear(d_model, output_dim)\n",
        "\n",
        "    def forward(self, x, attention_mask = None):\n",
        "        \"\"\"\n",
        "        Args:\n",
        "            x: Input tensor of shape (batch_size, seq_len, input_dim).\n",
        "        Returns:\n",
        "            Encoded vector of shape (batch_size, output_dim).\n",
        "        \"\"\"\n",
        "        # Project input to d_model dimension\n",
        "        x = self.input_projection(x)  # Shape: (batch_size, seq_len, d_model)\n",
        "\n",
        "        # Pass through transformer encoder layers\n",
        "        for layer in self.encoder:\n",
        "            x = layer(x,attention_mask)  # Shape: (batch_size, seq_len, d_model)\n",
        "\n",
        "        # Pool across the sequence dimension\n",
        "        x = x.transpose(1, 2)  # Shape: (batch_size, d_model, seq_len)\n",
        "        x = self.pooling(x).squeeze(-1)  # Shape: (batch_size, d_model)\n",
        "\n",
        "        # Project to output_dim\n",
        "        output = self.fc_out(x)  # Shape: (batch_size, output_dim)\n",
        "        return output\n",
        "\n",
        "\n",
        "class TransformerLayer(nn.Module):\n",
        "    def __init__(self, d_model, n_heads, dim_feedforward):\n",
        "        \"\"\"\n",
        "        Single transformer encoder layer.\n",
        "        Args:\n",
        "            d_model: Dimension of token embeddings.\n",
        "            n_heads: Number of attention heads.\n",
        "            dim_feedforward: Dimension of feedforward layers.\n",
        "        \"\"\"\n",
        "        super(TransformerLayer, self).__init__()\n",
        "\n",
        "        # Multi-head self-attention\n",
        "        self.self_attn = nn.MultiheadAttention(embed_dim=d_model, num_heads=n_heads, batch_first=True)\n",
        "\n",
        "        # Feedforward network\n",
        "        self.feedforward = nn.Sequential(\n",
        "            nn.Linear(d_model, dim_feedforward),\n",
        "            nn.ReLU(),\n",
        "            nn.Linear(dim_feedforward, d_model)\n",
        "        )\n",
        "\n",
        "        # Layer normalization\n",
        "        self.norm1 = nn.LayerNorm(d_model)\n",
        "        self.norm2 = nn.LayerNorm(d_model)\n",
        "\n",
        "    def forward(self, x, attention_mask=None):\n",
        "        \"\"\"\n",
        "        Args:\n",
        "            x: Input tensor of shape (batch_size, seq_len, d_model).\n",
        "        Returns:\n",
        "            Output tensor of shape (batch_size, seq_len, d_model).\n",
        "        \"\"\"\n",
        "        # Self-attention\n",
        "        attn_output, _ = self.self_attn(x, x, x, key_padding_mask=attention_mask)  # Shape: (batch_size, seq_len, d_model)\n",
        "        x = x + attn_output  # Residual connection\n",
        "        x = self.norm1(x)    # Layer normalization\n",
        "\n",
        "        # Feedforward network\n",
        "        ff_output = self.feedforward(x)  # Shape: (batch_size, seq_len, d_model)\n",
        "        x = x + ff_output  # Residual connection\n",
        "        x = self.norm2(x)  # Layer normalization\n",
        "\n",
        "        return x\n"
      ]
    },
