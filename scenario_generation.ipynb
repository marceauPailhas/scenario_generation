        "def plot_actor_tracks(fig: plt.Figure,ax: plt.Axes, scenario: ArgoverseScenario, timestep: int) -> Optional[_PlotBounds]:\n",
        "    \"\"\"Plot all actor tracks (up to a particular time step) associated with an Argoverse scenario.\n",
        "\n",
        "    Args:\n",
        "        ax: Axes on which actor tracks should be plotted.\n",
        "        scenario: Argoverse scenario for which to plot actor tracks.\n",
        "        timestep: Tracks are plotted for all actor data up to the specified time step.\n",
        "\n",
        "    Returns:\n",
        "        track_bounds: (x_center, y_center) bounds for the extent of actor tracks.\n",
        "    \"\"\"\n",
        "\n",
        "    image_size = (4.5,4.5)#fig.get_size_inches() #width, height in inches\n",
        "    resolution = fig.dpi #resolution in dots per inch\n",
        "    #print(image_size, resolution)\n",
        "    delta_alpha_channels = np.zeros((int(image_size[0]*resolution), int(image_size[1]*resolution), 3*4), dtype=np.float64)\n",
        "    track_bounds = None\n",
        "\n",
        "    #Defines 1s-separated timesteps\n",
        "    before_timestep = [timestep -20, timestep -10 , timestep]\n",
        "    after_timestep = [timestep +10, timestep +20]\n",
        "\n",
        "    for track in scenario.tracks:\n",
        "        if track.category == TrackCategory.FOCAL_TRACK:\n",
        "            position: NDArrayFloat = np.array([list(object_state.position) for object_state in track.object_states\n",
        "                                  if object_state.timestep == timestep ])\n",
        "            if position.shape[0] < 1:\n",
        "                raise ValueError(f\"Focal track not found at timestep {timestep}\")\n",
        "            x_center = position[0,0]\n",
        "            y_center = position[0,1]\n",
        "            track_bounds = (x_center, y_center)\n",
        "            break\n",
        "\n",
        "    for track in scenario.tracks:\n",
        "        if track.object_type in _STATIC_OBJECT_TYPES:\n",
        "            continue\n",
        "        # Get timesteps for which actor data is valid\n",
        "        actor_timesteps: NDArrayInt = np.array(\n",
        "            [object_state.timestep for object_state in track.object_states if object_state.timestep in before_timestep]\n",
        "        )\n",
        "        if actor_timesteps.shape[0] < 1 or actor_timesteps[-1] != timestep:\n",
        "            continue\n",
        "\n",
        "\n",
        "        # Get actor trajectory and heading history\n",
        "        actor_trajectory: NDArrayFloat = np.array(\n",
        "            [list(object_state.position) for object_state in track.object_states\n",
        "                                  if object_state.timestep in before_timestep ]\n",
        "        )\n",
        "        if max ( abs(actor_trajectory[-1,0]-x_center), abs(actor_trajectory[-1,1] - y_center) ) \\\n",
        "                          > _PLOT_BOUNDS_BUFFER_M :\n",
        "            continue\n",
        "\n",
        "        actor_headings: NDArrayFloat = np.array(\n",
        "            [object_state.heading for object_state in track.object_states\n",
        "                                  if object_state.timestep in before_timestep ]\n",
        "        )\n",
        "        # future_headings: NDArrayFloat = np.array(\n",
        "        #     [object_state.heading for object_state in track.object_states\n",
        "        #                           if object_state.timestep in after_timestep ]\n",
        "        # )\n",
        "\n",
        "        # Plot polyline for focal agent location history\n",
        "        track_color = _DEFAULT_ACTOR_COLOR\n",
        "\n",
        "        # Plot bounding boxes for all vehicles and cyclists\n",
        "        if track.object_type == ObjectType.VEHICLE:\n",
        "            mask = plot_actor_bounding_box(\n",
        "                ax,\n",
        "                actor_trajectory[-1],\n",
        "                actor_headings[-1],\n",
        "                track_color,\n",
        "                (_ESTIMATED_VEHICLE_LENGTH_M, _ESTIMATED_VEHICLE_WIDTH_M),\n",
        "                image_size,\n",
        "                resolution,\n",
        "                (x_center,y_center),\n",
        "\n",
        "            )\n",
        "\n",
        "        elif track.object_type == ObjectType.BUS :\n",
        "            mask = plot_actor_bounding_box(\n",
        "                ax,\n",
        "                actor_trajectory[-1],\n",
        "                actor_headings[-1],\n",
        "                _BUS_COLOR,\n",
        "                (_ESTIMATED_BUS_LENGTH_M, _ESTIMATED_BUS_WIDTH_M),\n",
        "                image_size,\n",
        "                resolution,\n",
        "                (x_center,y_center),\n",
        "            )\n",
        "\n",
        "        elif track.object_type == ObjectType.CYCLIST or track.object_type == ObjectType.MOTORCYCLIST:\n",
        "            mask = plot_actor_bounding_box(\n",
        "                ax,\n",
        "                actor_trajectory[-1],\n",
        "                actor_headings[-1],\n",
        "                track_color,\n",
        "                (_ESTIMATED_CYCLIST_LENGTH_M, _ESTIMATED_CYCLIST_WIDTH_M),\n",
        "                image_size,\n",
        "                resolution,\n",
        "                (x_center,y_center),\n",
        "            )\n",
        "\n",
        "        else:\n",
        "            plt.plot(actor_trajectory[-1, 0], actor_trajectory[-1, 1], \"o\", color=track_color, markersize=4)\n",
        "\n",
        "        if track.object_type in [ObjectType.VEHICLE, ObjectType.BUS, ObjectType.CYCLIST, ObjectType.MOTORCYCLIST]:\n",
        "            #Defines future timesteps and trajectories to compute alpha and delta\n",
        "            future_timesteps: NDArrayInt = np.array(\n",
        "            [object_state.timestep for object_state in track.object_states if object_state.timestep in after_timestep]\n",
        "              )\n",
        "\n",
        "            future_trajectory: NDArrayFloat = np.array(\n",
        "            [list(object_state.position) for object_state in track.object_states\n",
        "                                  if object_state.timestep in after_timestep ]\n",
        "              )\n",
        "            # Compute position differences (t)\n",
        "            #print('shape',actor_trajectory.shape)\n",
        "            past_deltas = np.diff(actor_trajectory, axis=0)\n",
        "\n",
        "            past_deltas = np.concatenate([np.zeros((2-past_deltas.shape[0] ,actor_trajectory.shape[1]),float ), past_deltas], axis =0)\n",
        "            #print(future_trajectory)\n",
        "            if future_trajectory.shape[0] < 1:\n",
        "                future_trajectory = np.zeros((0,2),float)\n",
        "\n",
        "            future_deltas = np.diff(np.concatenate([actor_trajectory[-1,:][np.newaxis ,:], future_trajectory], axis=0)\n",
        "                                    ,\n",
        "                                    axis=0 )\n",
        "            #print(future_deltas)\n",
        "            future_deltas = np.concatenate(\n",
        "                                  [future_deltas, np.zeros((2-future_deltas.shape[0] ,actor_trajectory.shape[1]), float)]\n",
        "                                  ,\n",
        "                                           axis =0)\n",
        "            deltas = np.concatenate([past_deltas, future_deltas], axis=0)\n",
        "            deltas = np.clip(deltas/30 +0.5, 0,1)\n",
        "            # # Compute mask conjunctions (t)\n",
        "            alphas = np.array([0]*(2-len(actor_timesteps) +1 )+[1]*(len(actor_timesteps) -1 ) \\\n",
        "                              + [1]*len(future_timesteps) + [0]*(2-len(future_timesteps) ) )\n",
        "\n",
        "            #(width, height, channels) set new channel\n",
        "            #print('shapes of alpha, delta', deltas.shape, alphas.shape)\n",
        "            alphas_and_deltas = (np.concatenate([deltas, alphas[:,np.newaxis]], axis = 1 ))\n",
        "            #print('alphasanddelas shepe', alphas_and_deltas.shape)\n",
        "            delta_alpha_channels[mask,:] = alphas_and_deltas.flatten()\n",
        "            #mask_sum += mask\n",
        "\n",
        "    return track_bounds, np.moveaxis(delta_alpha_channels,0,1)[::-1,:,:]\n",
        "\n"
      ]
    },
