      "source": [
        "def plot_actor_trajectories(scenario: ArgoverseScenario, timestep: int, ifsave: bool = True) -> None:\n",
        "    \"\"\"Plot all actor tracks associated with an Argoverse scenario.\n",
        "\n",
        "    Args:\n",
        "        scenario: Argoverse scenario for which to plot actor positions.\n",
        "        timestep: Tracks are plotted for all actor data at the specified time step.\n",
        "    \"\"\"\n",
        "\n",
        "    fig, ax = plt.subplots()\n",
        "\n",
        "    cur_plot_bounds, mask_sum = plot_actor_tracks(fig, ax, scenario, timestep)\n",
        "    if cur_plot_bounds:\n",
        "        plot_bounds = cur_plot_bounds\n",
        "\n",
        "    # Set map bounds to capture focal trajectory history (with fixed buffer in all directions)\n",
        "    plt.xlim(\n",
        "        plot_bounds[0] - _PLOT_BOUNDS_BUFFER_M,\n",
        "        plot_bounds[0] + _PLOT_BOUNDS_BUFFER_M,\n",
        "    )\n",
        "    plt.ylim(\n",
        "        plot_bounds[1] - _PLOT_BOUNDS_BUFFER_M,\n",
        "        plot_bounds[1] + _PLOT_BOUNDS_BUFFER_M,\n",
        "    )\n",
        "    plt.gca().set_aspect(\"equal\", adjustable=\"box\")\n",
        "\n",
        "    if ifsave:\n",
        "        # Adjust the axis and remove extra space\n",
        "        #ax.axis(\"equal\")\n",
        "        ax.axis(\"off\")  # Turn off the axis\n",
        "        plt.tight_layout()\n",
        "\n",
        "        # Save the figure to a buffer\n",
        "        buffer = io.BytesIO()\n",
        "        plt.savefig(buffer, format='png', bbox_inches='tight', pad_inches=0)\n",
        "        buffer.seek(0)\n",
        "\n",
        "        # Convert the buffer to a NumPy array\n",
        "        image = np.array(Image.open(buffer))\n",
        "\n",
        "        # Close the plot and buffer\n",
        "        plt.close(fig)\n",
        "        buffer.close()\n",
        "        return image,mask_sum\n"
      ]
    },
