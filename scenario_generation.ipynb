        "class ReconstructionLoss(torch.nn.Module):\n",
        "    def __init__(self, device , matching_weights=None, loss_weights=None):\n",
        "        \"\"\"\n",
        "        Custom loss function combining classification and regression losses\n",
        "        as described. Allows configurable weights for different loss components.\n",
        "        \"\"\"\n",
        "        super(ReconstructionLoss, self).__init__()\n",
        "        self.l1_loss = torch.nn.L1Loss()\n",
        "        self.mse_loss = torch.nn.MSELoss()  # For L2 loss\n",
        "        self.bce_loss = torch.nn.BCEWithLogitsLoss(reduction = 'none')  # Classification loss\n",
        "        self.cosine_loss = torch.nn.CosineSimilarity(dim=0)\n",
        "\n",
        "        # Set weights for each loss term (default values provided)\n",
        "        self.matching_weights = matching_weights or {\n",
        "            'classification': 4.0,\n",
        "            'l1_box_params': 1.0,\n",
        "            'l2_box_vertices': 1.0,\n",
        "        }\n",
        "\n",
        "        self.loss_weights = loss_weights or {\n",
        "            'classification': 20.0,\n",
        "            'l1_box_params': 1.0,\n",
        "            'l2_box_vertices': 1.0,\n",
        "            'l2_trajectory_positions': 1.0,\n",
        "            'cosine_trajectory_headings': 1.0,\n",
        "        }\n",
        "\n",
        "        self.motion_profile_weights = torch.tensor([0.1, 0.3, 4.0], device=device) # stationary,straight, or turning\n",
        "\n",
        "        self.device = device\n",
        "\n",
        "\n",
        "\n",
        "    def forward(self, predictions, targets):\n",
        "        \"\"\"\n",
        "        Args:\n",
        "            predictions: A tuple containing:\n",
        "                - box_parameters_pred: Predicted box parameters\n",
        "                - box_vertices_pred: Predicted box vertices\n",
        "                - trajectory_positions_pred: Predicted trajectory positions\n",
        "                - trajectory_headings_pred: Predicted trajectory headings\n",
        "                - classification_pred: Predicted classification logits\n",
        "            targets: A tuple containing:\n",
        "                - box_parameters_target: Ground truth box parameters\n",
        "                - box_vertices_target: Ground truth box vertices\n",
        "                - trajectory_positions_target: Ground truth trajectory positions\n",
        "                - trajectory_headings_target: Ground truth trajectory headings\n",
        "        \"\"\"\n",
        "        #print(predictions.shape, targets.shape)\n",
        "        (classification_pred, box_parameters_pred , trajectory_positions_pred,\n",
        "         trajectory_headings_pred ) = predictions[0,:,:].to(self.device), predictions[1:7,:,:].to(self.device), \\\n",
        "                                    torch.cat((predictions[8::3,:,:], predictions[9::3,:,:]), dim=0).to(self.device), \\\n",
        "                                    predictions[10::3,:,:].to(self.device)\n",
        "\n",
        "        box_vertices_pred = [\n",
        "              box_parameters_pred[0,:,:]*torch.exp(box_parameters_pred[5,:,:]) - box_parameters_pred[1,:,:]*torch.exp(box_parameters_pred[2,:,:]),\n",
        "              #fr_x = dr *cos(theta) - df * sin(theta)\n",
        "              box_parameters_pred[1,:,:]*torch.exp(box_parameters_pred[5,:,:]) + box_parameters_pred[0,:,:]*torch.exp(box_parameters_pred[2,:,:]) ,\n",
        "              #fr_y = dr *sin(theta) + df * cos(theta)\n",
        "              -box_parameters_pred[0,:,:]*torch.exp(box_parameters_pred[3,:,:]) - box_parameters_pred[1,:,:]*torch.exp(box_parameters_pred[2,:,:]),\n",
        "              #fl_x = -dl *cos(theta) - df * sin(theta)\n",
        "              -box_parameters_pred[1,:,:]*torch.exp(box_parameters_pred[3,:,:]) + box_parameters_pred[0,:,:]*torch.exp(box_parameters_pred[2,:,:]) ,\n",
        "              #fl_y = -dl *sin(theta) + df * cos(theta)\n",
        "              -box_parameters_pred[0,:,:]*torch.exp(box_parameters_pred[3,:,:]) + box_parameters_pred[1,:,:]*torch.exp(box_parameters_pred[4,:,:]),\n",
        "              #bl_x = -dl *cos(theta) + db * sin(theta)\n",
        "              -box_parameters_pred[1,:,:]*torch.exp(box_parameters_pred[3,:,:]) - box_parameters_pred[0,:,:]*torch.exp(box_parameters_pred[4,:,:]) ,\n",
        "              #bl_y = -dl *sin(theta) - db * cos(theta)\n",
        "              box_parameters_pred[0,:,:]*torch.exp(box_parameters_pred[5,:,:]) + box_parameters_pred[1,:,:]*torch.exp(box_parameters_pred[4,:,:]),\n",
        "              #br_x = dr *cos(theta) + db * sin(theta)\n",
        "              box_parameters_pred[1,:,:]*torch.exp(box_parameters_pred[5,:,:]) - box_parameters_pred[0,:,:]*torch.exp(box_parameters_pred[4,:,:]) ,\n",
        "              #br_y = dr *sin(theta) - db * cos(theta)\n",
        "                                        ]\n",
        "        box_vertices_pred = torch.cat ( [tensor.unsqueeze(0) for tensor in box_vertices_pred]   , dim =0).to(self.device)\n",
        "        #f=front, r=right, l=left, b=back\n",
        "        x = torch.linspace(\n",
        "                        0,\n",
        "                        2*_PLOT_BOUNDS_BUFFER_M - 4*_PLOT_BOUNDS_BUFFER_M/box_vertices_pred.shape[-1] ,\n",
        "                        box_vertices_pred.shape[-1],\n",
        "                        device = self.device\n",
        "                        )\n",
        "\n",
        "        mesh_x, mesh_y = torch.meshgrid(x, x , indexing='ij')\n",
        "        pixel_centers = torch.stack((mesh_y, mesh_x), dim=0) + torch.tensor(_PLOT_BOUNDS_BUFFER_M/box_vertices_pred.shape[-1]).to(self.device)\n",
        "        # print('shapes are', pixel_centers.shape, box_vertices_pred.shape)\n",
        "\n",
        "        box_vertices_pred = box_vertices_pred + torch.cat([pixel_centers]*4 , dim = 0).to(self.device)\n",
        "\n",
        "        targets = targets.to(self.device)\n",
        "        ## remove zero values in targets\n",
        "        targets = targets[~torch.all(targets == 0, dim=1)]\n",
        "\n",
        "        (box_current_position, box_parameters_target , trajectory_positions_target,\n",
        "         trajectory_headings_target ) = targets[:,:2], targets[:,2:8], \\\n",
        "                                    torch.cat((targets[:,9::3], targets[:,10::3]), dim=1), \\\n",
        "                                    targets[:,11::3]\n",
        "\n",
        "        box_vertices_target = [\n",
        "              box_parameters_target[:,0]*torch.exp(box_parameters_target[:,5]) - box_parameters_target[:,1]*torch.exp(box_parameters_target[:,2]),\n",
        "              #fr_x = dr *cos(theta) - df * sin(theta)\n",
        "              box_parameters_target[:,1]*torch.exp(box_parameters_target[:,5]) + box_parameters_target[:,0]*torch.exp(box_parameters_target[:,2]) ,\n",
        "              #fr_y = dr *sin(theta) + df * cos(theta)\n",
        "              -box_parameters_target[:,0,]*torch.exp(box_parameters_target[:,3]) - box_parameters_target[:,1]*torch.exp(box_parameters_target[:,2]),\n",
        "              #fl_x = -dl *cos(theta) - df * sin(theta)\n",
        "              -box_parameters_target[:,1]*torch.exp(box_parameters_target[:,3]) + box_parameters_target[:,0]*torch.exp(box_parameters_target[:,2]) ,\n",
        "              #fl_y = -dl *sin(theta) + df * cos(theta)\n",
        "              -box_parameters_target[:,0]*torch.exp(box_parameters_target[:,3]) + box_parameters_target[:,1]*torch.exp(box_parameters_target[:,4]),\n",
        "              #bl_x = -dl *cos(theta) + db * sin(theta)\n",
        "              -box_parameters_target[:,1]*torch.exp(box_parameters_target[:,3]) - box_parameters_target[:,0]*torch.exp(box_parameters_target[:,4]) ,\n",
        "              #bl_y = -dl *sin(theta) - db * cos(theta)\n",
        "              box_parameters_target[:,0]*torch.exp(box_parameters_target[:,5]) + box_parameters_target[:,1]*torch.exp(box_parameters_target[:,4]),\n",
        "              #br_x = dr *cos(theta) + db * sin(theta)\n",
        "              box_parameters_target[:,1]*torch.exp(box_parameters_target[:,5]) - box_parameters_target[:,0]*torch.exp(box_parameters_target[:,4]) ,\n",
        "              #br_y = dr *sin(theta) - db * cos(theta)\n",
        "                                        ]\n",
        "        box_vertices_target = torch.cat ( [tensor.unsqueeze(1) for tensor in box_vertices_target] , dim =1).to(self.device)\n",
        "        #print(box_vertices_target.shape)\n",
        "\n",
        "        #f=front, r=right, l=left, b=back, probably add the pixel center\n",
        "        box_current_position  = box_current_position + _PLOT_BOUNDS_BUFFER_M\n",
        "        box_vertices_target = box_vertices_target + torch.cat([box_current_position]*4, dim = 1 )\n",
        "\n",
        "        #classify trajectories as straight, turning, immobile\n",
        "        moving = torch.linalg.norm (torch.sum(trajectory_positions_target.view(targets.shape[0],-1,2),dim = 1 ), dim=1 ) > 10\n",
        "        turning = torch.abs ( torch.sum(trajectory_headings_target,dim = 1 ) ) > 3.14/4\n",
        "        #if not moving = immobile(0), if moving but not turning = straight(1), if moving and turning = turning(2)\n",
        "        trajectory_category = moving*(1 + turning)\n",
        "\n",
        "        classification_target  = torch.zeros((predictions.shape[-2:]), device=self.device)\n",
        "\n",
        "        box_current_position_in_pixels = box_current_position* (predictions.shape[-1]/(2*_PLOT_BOUNDS_BUFFER_M))\n",
        "        #print(box_current_position_in_pixels)\n",
        "        for n in range(box_current_position_in_pixels.shape[0]):\n",
        "            classification_target[tuple(box_current_position_in_pixels[n, :].to(dtype=torch.int64))] = 1\n",
        "\n",
        "        classification_pred =  torch.sigmoid(classification_pred)\n",
        "\n",
        "        ## Matching\n",
        "        ## 256 * 256 * n matrix\n",
        "        #classification loss\n",
        "        classification_loss = torch.zeros((predictions.shape[-2],predictions.shape[-1],targets.shape[0]), device=self.device)\n",
        "\n",
        "        for n in range(box_current_position_in_pixels.shape[0]):\n",
        "            #print(tuple(box_current_position_in_pixels[n, :].to(dtype=torch.int64)),\n",
        "                  #classification_pred[tuple(box_current_position_in_pixels[n, :].to(dtype=torch.int64))])\n",
        "            classification_loss[tuple(box_current_position_in_pixels[n, :].to(dtype=torch.int64)),n] = \\\n",
        "                          self.bce_loss(\n",
        "                                  classification_pred[tuple(box_current_position_in_pixels[n, :].to(dtype=torch.int64))]\n",
        "                                  ,\n",
        "                                  torch.tensor(1., device=self.device)\n",
        "                              )\n",
        "        ##coordinate loss\n",
        "        coordinate_loss = torch.tensor([\n",
        "            [\n",
        "              [\n",
        "                  self.l1_loss(box_parameters_pred[:,x,y], box_parameters_target[n,:])\n",
        "                  for n in range(targets.shape[0])  ]\n",
        "          for y in range(predictions.shape[-1])   ]\n",
        "        for x in range(predictions.shape[-2])\n",
        "        ], device=self.device  )\n",
        "        ## vertex loss\n",
        "        vertex_loss = torch.tensor([\n",
        "            [\n",
        "              [\n",
        "                  self.mse_loss(box_vertices_pred[:,x,y], box_vertices_target[n,:])\n",
        "                  for n in range(targets.shape[0])  ]\n",
        "          for y in range(predictions.shape[-1])   ]\n",
        "        for x in range(predictions.shape[-2])\n",
        "        ] ,  device=self.device )\n",
        "\n",
        "        matching_loss = self.matching_weights['classification']*classification_loss + \\\n",
        "                        self.matching_weights['l1_box_params']*coordinate_loss + \\\n",
        "                        self.matching_weights['l2_box_vertices']*vertex_loss\n",
        "\n",
        "        #print(matching_loss.shape)\n",
        "        indices_flat = torch.argmin(matching_loss.view(-1, matching_loss.shape[-1]), dim=0)\n",
        "        #print(indices_flat)\n",
        "        indices_2d = torch.div(indices_flat, matching_loss.shape[0], rounding_mode='floor'), indices_flat % matching_loss.shape[0]\n",
        "        #print(indices_2d)\n",
        "        matched_indices2D = list(zip(indices_2d[0].tolist(), indices_2d[1].tolist()))\n",
        "\n",
        "        ## Compute loss with matched bounding boxes as ground truths ##\n",
        "        # Initialize classification loss, coordinate loss, vertex loss, trajectory loss, heading loss\n",
        "        classification_loss = 0\n",
        "        l1_box_params_loss = 0\n",
        "        l2_box_vertices_loss = 0\n",
        "        l2_trajectory_positions_loss = 0\n",
        "        cosine_heading_loss = 0\n",
        "\n",
        "\n",
        "\n",
        "        for n in range(targets.shape[0]):\n",
        "\n",
        "              classification_loss += self.bce_loss(\n",
        "                  classification_pred[matched_indices2D[n]],\n",
        "                  torch.tensor(1., device=self.device)\n",
        "                  )\n",
        "\n",
        "              # Compute regression losses\n",
        "              width_ind , height_ind = matched_indices2D[n]\n",
        "              # print(matched_indices2D[n])\n",
        "              # print(box_parameters_pred[:,width_ind , height_ind].shape,\n",
        "              #      box_parameters_target[n,:].shape )\n",
        "              l1_box_params_loss += self.l1_loss(box_parameters_pred[:,width_ind , height_ind], box_parameters_target[n,:])\n",
        "              l2_box_vertices_loss += self.mse_loss(box_vertices_pred[:,width_ind , height_ind], box_vertices_target[n,:])\n",
        "              l2_trajectory_positions_loss += self.motion_profile_weights[int(trajectory_category[n])] * \\\n",
        "                self.mse_loss(\n",
        "                  trajectory_positions_pred[:,width_ind , height_ind], trajectory_positions_target[n,:]\n",
        "                  )\n",
        "\n",
        "              # Compute cosine similarity loss for trajectory headings\n",
        "              cosine_heading_loss += self.motion_profile_weights[int(trajectory_category[n])] * \\\n",
        "              ( 1 - self.cosine_loss(\n",
        "                  trajectory_headings_pred[:,width_ind , height_ind], trajectory_headings_target[n,:]\n",
        "                  )\n",
        "              )\n",
        "\n",
        "        ##BCE loss on unmatched bounding boxes\n",
        "        #print(classification_pred.shape)\n",
        "        unmatched_classification_loss = self.bce_loss(classification_pred,torch.zeros(classification_pred.shape, device=self.device)) #(size H,W)\n",
        "        weights_unmacthed = torch.tensor([\n",
        "    [\n",
        "        0 if (x, y) in matched_indices2D else\n",
        "        (0.2 if torch.min(torch.linalg.norm ( torch.tensor(matched_indices2D,dtype=torch.float64) - torch.tensor([x, y],dtype=torch.float64), dim =1)) <= 3 else 0.002)\n",
        "        for y in range(predictions.shape[-1])\n",
        "    ]\n",
        "    for x in range(predictions.shape[-2])\n",
        "], device=self.device)\n",
        "        classification_loss += torch.sum(unmatched_classification_loss * weights_unmacthed)\n",
        "\n",
        "        # Weighted combination of all losses\n",
        "        total_loss = (\n",
        "            self.loss_weights['classification'] * classification_loss +\n",
        "            self.loss_weights['l1_box_params'] * l1_box_params_loss +\n",
        "            self.loss_weights['l2_box_vertices'] * l2_box_vertices_loss +\n",
        "            self.loss_weights['l2_trajectory_positions'] * l2_trajectory_positions_loss +\n",
        "            self.loss_weights['cosine_trajectory_headings'] * cosine_heading_loss\n",
        "        )\n",
        "\n",
        "        return total_loss\n",
        "\n",
        "\n",
        "##Matching\n",
        "# To match a predicted box to a given ground truth box we select the predicted box that minimizes a\n",
        "# weighted combination of the classification, coordinate, and vertex losses.\n",
        "\n",
        "##LOsses\n",
        "# categorize ground truth trajectory\n",
        "# stationnary, straight, turning by 0.1, 0.3, and 4, respectively.\n",
        "\n",
        "# binary cross-entropy loss on the unmatched predicted boxes to have probability 0.\n",
        "# Negative boxes < 3 m of grd truth box : factor 0.2,\n",
        "# negative boxes > 3 m from a grnd truth box : factor of 0.002."
