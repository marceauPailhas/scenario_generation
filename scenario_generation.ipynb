        "# Resampling function\n",
        "def resample_and_split_scenario(scenario: ArgoverseScenario) -> Tuple[ArgoverseScenario, ArgoverseScenario]:\n",
        "    interval_ns = 1e9\n",
        "    resampled_timestamps_ns = np.arange(\n",
        "        scenario.timestamps_ns[0], scenario.timestamps_ns[-1] + interval_ns, interval_ns\n",
        "    )\n",
        "\n",
        "    # Get position of the AV at time 2 secs, 7 secs\n",
        "    av_position_at_2 = get_av_position(scenario, 2)\n",
        "    av_position_at_7 = get_av_position(scenario, 7)\n",
        "\n",
        "\n",
        "    # Resample tracks\n",
        "    resampled_tracks_first_split = []\n",
        "    resampled_tracks_second_split = []\n",
        "\n",
        "    for track in scenario.tracks:\n",
        "        resampled_states_ = [\n",
        "            state for state in track.object_states\n",
        "                        if state.timestep in range(0,110,10)\n",
        "        ]\n",
        "\n",
        "        resampled_states_at_2 = [state for state in resampled_states_\n",
        "                                    if state.timestep  == 20]\n",
        "        resampled_states_at_7 = [state for state in resampled_states_\n",
        "                                    if state.timestep  == 70]\n",
        "\n",
        "        if len(resampled_states_at_2) >1:\n",
        "            raise TypeError('Too many state at the same timestep for the same vehicule')\n",
        "        elif len(resampled_states_at_2) ==1:\n",
        "            if np.max ( abs (np.array(resampled_states_at_2[0].position) - np.array(av_position_at_2)) ) <= 50 :\n",
        "\n",
        "              resampled_tracks_first_split.append(\n",
        "                  Track(\n",
        "                      track_id=track.track_id,\n",
        "                      object_states= [state for state in resampled_states_\n",
        "                                                  if state.timestep  < 50],\n",
        "                      object_type=track.object_type,\n",
        "                      category=track.category,\n",
        "                      )\n",
        "                  )\n",
        "        if len(resampled_states_at_7) >1:\n",
        "              raise TypeError('Too many state at the same timestep for the same vehicule')\n",
        "        elif len(resampled_states_at_7) ==1:\n",
        "            if np.max ( abs(np.array(resampled_states_at_7[0].position) - np.array(av_position_at_7)) ) <= 50 :\n",
        "                resampled_tracks_second_split.append(\n",
        "                    Track(\n",
        "                        track_id=track.track_id,\n",
        "                        object_states=[state for state in resampled_states_\n",
        "                                                    if state.timestep  > 50],\n",
        "                        object_type=track.object_type,\n",
        "                        category=track.category,\n",
        "                    )\n",
        "                )\n",
        "\n",
        "    return ArgoverseScenario(\n",
        "        scenario_id=scenario.scenario_id,\n",
        "        timestamps_ns=resampled_timestamps_ns[:5],\n",
        "        tracks=resampled_tracks_first_split,\n",
        "        focal_track_id=scenario.focal_track_id,\n",
        "        city_name=scenario.city_name,\n",
        "        map_id=scenario.map_id,\n",
        "        slice_id=scenario.slice_id,\n",
        "    ) , \\\n",
        "    ArgoverseScenario(\n",
        "        scenario_id=scenario.scenario_id,\n",
        "        timestamps_ns=resampled_timestamps_ns[6:],\n",
        "        tracks=resampled_tracks_second_split,\n",
        "        focal_track_id=scenario.focal_track_id,\n",
        "        city_name=scenario.city_name,\n",
        "        map_id=scenario.map_id,\n",
        "        slice_id=scenario.slice_id,\n",
        "    )\n",
        "\n",
        "\n",
        "\n",
        "# Function to get position of the autonomous vehicle at a specific second\n",
        "def get_av_position(scenario: ArgoverseScenario, timestamp_s: int) -> Tuple[float, float]:\n",
        "    focal_track = next(\n",
        "        (track for track in scenario.tracks if track.track_id == scenario.focal_track_id), None\n",
        "    )\n",
        "    if focal_track is None:\n",
        "        return None\n",
        "\n",
        "    state_at_time = next(\n",
        "        (\n",
        "            state for state in focal_track.object_states\n",
        "                                    if state.timestep == timestamp_s*10\n",
        "         ), None\n",
        "    )\n",
        "    return state_at_time.position if state_at_time else None\n",
        "\n",
        "\n"
      ]
    },
