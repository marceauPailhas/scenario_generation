        "import torch\n",
        "import numpy as np\n",
        "from shapely.geometry import Polygon\n",
        "\n",
        "def inference_reverse_process(\n",
        "    Epsilon ,\n",
        "    Dup ,\n",
        "    Ddown ,\n",
        "    Epsilon_m ,\n",
        "    MLP,\n",
        "    M,  # Model M(z; c, )\n",
        "    context,  # Context c\n",
        "    sigma_max=1.0,\n",
        "    sigma_min=0.01,\n",
        "    num_steps=100,\n",
        "    threshold=0.8,\n",
        "    sigma_data=0.5,\n",
        "    device='cpu'\n",
        "):\n",
        "    \"\"\"\n",
        "    Perform inference using the reverse process ODE.\n",
        "\n",
        "    Args:\n",
        "        Epsilon (torch.nn.Module) : encodes the vehicules trajectories,\n",
        "        Dup (torch.nn.Module) : decoder in the autoencoder,\n",
        "        Ddown (torch.nn.Module) : encodes the map in the autoendecoder,\n",
        "        Epsilon_m (torch.nn.Module) : encodes the map in the diffusion,\n",
        "        MLP (torch.nn.Module) : The NN that takes tokens ,\n",
        "        M (torch.nn.Module): The UNet neural network approximating M(z; c, ).\n",
        "        decoder_D (torch.nn.Module): The decoder to generate final outputs.\n",
        "        context (torch.Tensor): The context c.\n",
        "        sigma_max (float): Maximum  value.\n",
        "        sigma_min (float): Minimum  value.\n",
        "        num_steps (int): Number of steps for Euler integration.\n",
        "        threshold (float): Probability threshold for filtering boxes.\n",
        "        device (str): The device to perform inference ('cpu' or 'cuda').\n",
        "\n",
        "    Returns:\n",
        "        Generated boxes and trajectories after filtering.\n",
        "    \"\"\"\n",
        "    # Step 1: Initialize z ~ N(0, sigma_max^2 I)\n",
        "    z = torch.randn(context.size(0), context.size(1), device=device) * sigma_max\n",
        "    m,tokens = context[0], context[1]\n",
        "    key , value = MLP(tokens)\n",
        "    m_latent = Epsilon_m(m)\n",
        "    # Step 2: Set up sigma schedule\n",
        "    sigmas = torch.linspace(sigma_max, sigma_min, num_steps, device=device)\n",
        "\n",
        "    # Step 3: Reverse process ODE with Euler integration\n",
        "    for sigma in sigmas:\n",
        "        sigma = sigma.item()  # Current sigma value as scalar\n",
        "\n",
        "        c_out = sigma*sigma_data/torch.sqrt(sigma**2+sigma_data**2)\n",
        "        c_in = 1 / torch.sqrt(sigma**2+sigma_data**2)\n",
        "        c_skip = sigma_data**2 / (sigma**2+sigma_data**2)\n",
        "\n",
        "        Mz = c_skip*z + c_out *M(torch.cat([c_in*z, m_latent], dim=1), key, value )\n",
        "\n",
        "        dz_dsigma = (z - Mz) / sigma\n",
        "        z = z - dz_dsigma * (sigma_max - sigma_min) / num_steps\n",
        "\n",
        "    # Step 4: Decode z to generate boxes and trajectories\n",
        "    boxes_and_trajectories = Dup(torch.cat([z, Ddown(m)], dim = 1))\n",
        "\n",
        "    # To review from here\n",
        "    # Step 5: Filter boxes by probability threshold\n",
        "\n",
        "    keep_indices = boxes_and_trajectories[:,0,:,:] > threshold\n",
        "    filtered_boxes = torch.zeros_like(boxes_and_trajectories)\n",
        "    filtered_boxes[keep_indices] = boxes_and_trajectories[keep_indices]\n",
        "\n",
        "    # Step 6: Remove overlapping boxes (keep highest probability)\n",
        "    filtered_boxes = remove_overlapping_boxes(filtered_boxes)\n",
        "\n",
        "    return filtered_boxes\n",
        "\n",
        "def remove_overlapping_boxes(boxes, pixel_center, pixel_width, pixel_height ):\n",
        "    \"\"\"\n",
        "    Remove overlapping boxes, keeping the one with the highest probability.\n",
        "\n",
        "    Args:\n",
        "        boxes (torch.Tensor): Tensor of bounding boxes.\n",
        "        pixel_height (torch.float): Height of a pixel in the image.\n",
        "        pixel_width (torch.float): Width of a pixel in the image.\n",
        "\n",
        "    Returns:\n",
        "        torch.Tensor: Filtered bounding boxes.\n",
        "    \"\"\"\n",
        "    box_parameters_pred = boxes[:,1:7,:,:]\n",
        "    box_vertices_pred = torch.cat ([\n",
        "              box_parameters_pred[:,0,:,:]*torch.exp(box_parameters_pred[:,5,:,:]) - box_parameters_pred[:,1,:,:]*torch.exp(box_parameters_pred[:,2,:,:]),\n",
        "              #fr_x = dr *cos(theta) - df * sin(theta)\n",
        "              box_parameters_pred[:,1,:,:]*torch.exp(box_parameters_pred[:,5,:,:]) + box_parameters_pred[:,0,:,:]*torch.exp(box_parameters_pred[:,2,:,:]) ,\n",
        "              #fr_y = dr *sin(theta) + df * cos(theta)\n",
        "              -box_parameters_pred[:,0,:,:]*torch.exp(box_parameters_pred[:,3,:,:]) - box_parameters_pred[:,1,:,:]*torch.exp(box_parameters_pred[:,2,:,:]),\n",
        "              #fl_x = -dl *cos(theta) - df * sin(theta)\n",
        "              -box_parameters_pred[:,1,:,:]*torch.exp(box_parameters_pred[:,3,:,:]) + box_parameters_pred[:,0,:,:]*torch.exp(box_parameters_pred[:,2,:,:]) ,\n",
        "              #fl_y = -dl *sin(theta) + df * cos(theta)\n",
        "              -box_parameters_pred[:,0,:,:]*torch.exp(box_parameters_pred[:,3,:,:]) + box_parameters_pred[:,1,:,:]*torch.exp(box_parameters_pred[:,4,:,:]),\n",
        "              #bl_x = -dl *cos(theta) + db * sin(theta)\n",
        "              -box_parameters_pred[:,1,:,:]*torch.exp(box_parameters_pred[:,3,:,:]) - box_parameters_pred[:,0,:,:]*torch.exp(box_parameters_pred[:,4,:,:]) ,\n",
        "              #bl_y = -dl *sin(theta) - db * cos(theta)\n",
        "              box_parameters_pred[:,0,:,:]*torch.exp(box_parameters_pred[:,5,:,:]) + box_parameters_pred[:,1,:,:]*torch.exp(box_parameters_pred[:,4,:,:]),\n",
        "              #br_x = dr *cos(theta) + db * sin(theta)\n",
        "              box_parameters_pred[:,1,:,:]*torch.exp(box_parameters_pred[:,5,:,:]) - box_parameters_pred[:,0,:,:]*torch.exp(box_parameters_pred[:,4,:,:]) ,\n",
        "              #br_y = dr *sin(theta) - db * cos(theta)\n",
        "                                        ]\n",
        "                                       , dim =1)\n",
        "\n",
        "    box_vertices_pred = torch.cat ([\n",
        "                                    torch.arange(0,boxes.size(2))*pixel_width +pixel_center[0],\n",
        "                                    torch.arange(0,boxes.size(3))*pixel_height+ pixel_center[1]\n",
        "                                    ]*4\n",
        "                                   , dim = 1 ) + box_vertices_pred\n",
        "\n",
        "    for batch_index in range(boxes.size(0)):\n",
        "      indices = torch.nonzero(boxes[batch_index, 0, :, :] != 0, as_tuple=False).tolist()\n",
        "\n",
        "      for index_i in range(len(indices)):\n",
        "        idx_i = indices[index_i]\n",
        "        if boxes[batch_index, 0, idx_i[0], idx_i[1]] == 0:\n",
        "          continue\n",
        "        for index_j in range(index_i + 1, len(indices)):\n",
        "            # Extract the indices for the two polygons\n",
        "            idx_j = indices[index_j]\n",
        "            if boxes[batch_index, 0, idx_j[0], idx_j[1]] == 0:\n",
        "              continue\n",
        "\n",
        "            # Define the polygons using the box vertices\n",
        "            poly1 = Polygon(box_vertices_pred[batch_index, :, idx_i[0], idx_i[1]].tolist())\n",
        "            poly2 = Polygon(box_vertices_pred[batch_index, :, idx_j[0], idx_j[1]].tolist())\n",
        "\n",
        "      if poly1.intersection(poly2).area > 0:\n",
        "        index_min = min(index_i,index_j, key = lambda x: boxes[batch_index,0,x[1], x[2]] )\n",
        "        boxes[batch_index,:,index_min[0],index_min[1]] = torch.zeros(boxes.size(1))\n",
        "    return boxes\n"
      ]
    }
