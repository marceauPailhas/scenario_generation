        "    )\n",
        "\n",
        "    for epoch in range(num_epochs):\n",
        "        total_loss = 0\n",
        "        for batch in train_loader:\n",
        "            x,m, x_label, attention_mask = batch['x'], batch['m'], batch[\"x_label\"], batch[\"attention_mask\"]\n",
        "            x = x.type(torch.float).to(device)\n",
        "            m = m.type(torch.float).to(device)\n",
        "            x_label = x_label.type(torch.float).to(device)\n",
        "            attention_mask = attention_mask.type(torch.bool).to(device)\n",
        "\n",
        "            optimizer.zero_grad()\n",
        "            # Forward pass\n",
        "\n",
        "            z = Epsilon(x)\n",
        "            sparse = Dup( torch.cat((Ddown(m),z), dim = 1) )\n",
        "\n",
        "            #Compute proxies for q(z), and standard normal\n",
