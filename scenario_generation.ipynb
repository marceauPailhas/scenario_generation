        "import torch.nn.functional as F\n",
        "\n",
        "\n",
        "class MLP(torch.nn.Module):\n",
        "    def __init__(self, input_dim, output_dim, hidden_dim=128):\n",
        "        super(MLP, self).__init__()\n",
        "        self.layers = torch.nn.Sequential(\n",
        "            torch.nn.Linear(input_dim, hidden_dim),\n",
        "            torch.nn.ReLU(),\n",
        "            torch.nn.Linear(hidden_dim, output_dim)\n",
        "        )\n",
        "\n",
        "    def forward(self, x):\n",
        "        return self.layers(x)\n",
        "\n",
        "\n",
        "class ResNetBlock(torch.nn.Module):\n",
        "    def __init__(self, in_channels, out_channels):\n",
        "        super(ResNetBlock, self).__init__()\n",
        "        self.in_channels = in_channels\n",
        "        self.out_channels = out_channels\n",
        "\n",
        "        self.conv1 = torch.nn.Conv2d(in_channels, out_channels, kernel_size=3, padding=1)\n",
        "        self.bn1 = torch.nn.BatchNorm2d(out_channels)\n",
        "        self.conv2 = torch.nn.Conv2d(out_channels, out_channels, kernel_size=3, padding=1)\n",
        "        self.bn2 = torch.nn.BatchNorm2d(out_channels)\n",
        "\n",
        "        self.identity = torch.nn.Conv2d(in_channels, out_channels, kernel_size=1, bias=False) \\\n",
        "            if in_channels != out_channels else torch.nn.Identity()\n",
        "          #change by true identity, using interpolation or\n",
        "    def forward(self, x):\n",
        "        out = F.relu(self.bn1(self.conv1(x)))\n",
        "        out = self.bn2(self.conv2(out))\n",
        "        identity = self.identity(x)\n",
        "        return F.relu(out + identity)\n",
        "\n",
        "\n",
        "class CrossAttention(torch.nn.Module):\n",
        "    def __init__(self, in_channels, out_channels, latent_dim = 64, num_heads=8):\n",
        "        super(CrossAttention, self).__init__()\n",
        "        self.num_heads = num_heads\n",
        "        self.in_channels = in_channels\n",
        "        self.out_channels = out_channels\n",
        "        self.query = torch.nn.Linear(in_channels, out_channels)\n",
        "\n",
        "        # Linear transformations for latent keys and values\n",
        "        self.key_transform = torch.nn.Linear(latent_dim, out_channels)\n",
        "        self.value_transform = torch.nn.Linear(latent_dim, out_channels)\n",
        "\n",
        "    def forward(self, x, latent_key, latent_value):\n",
        "        \"\"\"\n",
        "        Args:\n",
        "        - x: Input tensor of shape (B, C, H, W).\n",
        "        - latent_key: Processed key tensor of shape (n_tokens, out_channels).\n",
        "        - latent_value: Processed value tensor of shape (1, out_channels).\n",
        "        \"\"\"\n",
        "        B, C, H, W = x.shape\n",
        "        n_tokens, latent_dim = latent_key.shape\n",
        "\n",
        "        # Flatten spatial dimensions of x and apply query transformation\n",
        "        x_flat = x.view(B, C, -1).transpose(1, 2)  # Shape: (B, H*W, C)\n",
        "        Q = self.query(x_flat)  # Shape: (B, H*W, out_channels)\n",
        "\n",
        "        # Transform latent keys and values to out_channels\n",
        "        K = self.key_transform(latent_key)  # Shape: (n_tokens, out_channels)\n",
        "        V = self.value_transform(latent_value)  # Shape: (1, out_channels)\n",
        "\n",
        "        # Compute scaled dot-product attention\n",
        "        attention_scores = torch.einsum('bqc,mc->bqm', Q, K) / (self.out_channels ** 0.5)  # (B, H*W, n_tokens)\n",
        "        attention_probs = F.softmax(attention_scores, dim=-1)  # Shape: (B, H*W, n_tokens)\n",
        "        attended = torch.einsum('bqm,mo->bqo', attention_probs, V)  # Shape: (B, H*W, out_channels)\n",
        "\n",
        "        # Reshape back to spatial dimensions\n",
        "        attended = attended.transpose(1, 2).view(B, self.out_channels, H, W)\n",
        "        return attended\n",
        "\n",
        "\n",
        "\n",
        "class UNetBlock(torch.nn.Module):\n",
        "    def __init__(self, in_channels, out_channels, attention=False, num_heads=8):\n",
        "        super(UNetBlock, self).__init__()\n",
        "        self.resnet_block = ResNetBlock(in_channels, out_channels) #we'd like in channels and out channels\n",
        "        self.attention = attention\n",
        "        if self.attention:\n",
        "            self.cross_attention = CrossAttention(in_channels, out_channels, num_heads)\n",
        "\n",
        "    def forward(self, x, key=None, value = None):\n",
        "        x = self.resnet_block(x)\n",
        "        if self.attention and key is not None and value is not None:\n",
        "            x = self.cross_attention(x, key, value)\n",
        "        return x\n",
        "\n",
        "\n",
        "\n",
        "class UNet(torch.nn.Module):\n",
        "    def __init__(self):\n",
        "        super(UNet, self).__init__()\n",
        "        self.embed_map = torch.nn.Conv2d(256, 64, kernel_size=1) #because [epsilon(x), epsilon_m(m)] are input\n",
        "\n",
        "        # Encoder\n",
        "        self.encoder1 = UNetBlock(64, 128)  # 32x32 block\n",
        "        self.encoder2 = UNetBlock(128, 256, attention = True, num_heads=8)  # 16x16 block\n",
        "        self.encoder3 = UNetBlock(256, 256, attention = True, num_heads=8)  # 8x8 block\n",
        "\n",
        "        # Decoder\n",
        "        self.decoder2 = UNetBlock(256, 128, attention=True, num_heads=8) # 16x16 block\n",
        "        self.decoder1 = UNetBlock(128, 64) # 32x32 block # Final output layer (1 channel)\n",
        "\n",
        "        self.upsample = torch.nn.Upsample(scale_factor=2, mode='bilinear', align_corners=False)\n",
        "        self.final_conv = nn.Conv2d(64, 128, kernel_size=1)\n",
        "\n",
        "    def forward(self, x , key = None, value = None):\n",
        "        # Encoder Path\n",
        "        em = self.embed_map(x)  # Embedded map (64 channels)\n",
        "        e1 = self.encoder1(em) #32 pixels\n",
        "        e2 = self.encoder2(F.avg_pool2d(e1, kernel_size=2), key = None, value = None) #16 pix\n",
        "        e3 = self.encoder3(F.avg_pool2d(e2, kernel_size=2), key = None, value = None) #8 pix\n",
        "\n",
        "        # Decoder Path\n",
        "\n",
        "        d2 = self.decoder2(self.upsample(e3) + e2, key = None, value = None) #16 pix\n",
        "        d1 = self.decoder1(self.upsample(d2)+ e1 ) #32 pix\n",
        "        d1 = self.final_conv(d1)\n",
        "        return d1\n",
        "#32 pixel, 16 pixel, and 8 pixel blocks of M use 64, 128, and 256 channels\n",
        "#add upsample #and the channel demultiplexer #downsample is avg_pool 2d"
