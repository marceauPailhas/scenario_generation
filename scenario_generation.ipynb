        "import torch"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 34,
      "metadata": {
        "id": "XuIBV0EakmSN"
      },
      "outputs": [],
      "source": [
        "class Encoder(torch.nn.Module):\n",
        "    def __init__(self, input_channel):\n",
        "      super(Encoder, self).__init__()\n",
        "\n",
        "      self.relu = torch.nn.ReLU(inplace=True)\n",
        "      self.input_channel = input_channel\n",
        "\n",
        "      #resblock\n",
        "      ##The 256 pixel and 128 pixel blocks use 64 channels, and the 64 pixel and 32 pixel blocks use 128\n",
        "      ## channels. We use one Resnet block per resolution.\n",
        "      in_channels = 64\n",
        "      out_channels = 64\n",
        "      stride = 1\n",
        "\n",
        "      self.conv_res1_256 = torch.nn.Conv2d(in_channels, out_channels, kernel_size=3, stride=stride, padding=1, bias=True)\n",
        "      self.bn1_256 = torch.nn.BatchNorm2d(out_channels)\n",
        "      self.conv_res2_256 = torch.nn.Conv2d(out_channels, out_channels, kernel_size=3, stride=1, padding=1, bias=True)\n",
        "      self.bn2_256 = torch.nn.BatchNorm2d(out_channels)\n",
        "\n",
        "      self.conv_res1_128 = torch.nn.Conv2d(in_channels, out_channels, kernel_size=3, stride=stride, padding=1, bias=True)\n",
        "      self.bn1_128 = torch.nn.BatchNorm2d(out_channels)\n",
        "      self.conv_res2_128 = torch.nn.Conv2d(out_channels, out_channels, kernel_size=3, stride=1, padding=1, bias=True)\n",
        "      self.bn2_128 = torch.nn.BatchNorm2d(out_channels)\n",
        "\n",
        "      in_channels = 128\n",
        "      out_channels = 128\n",
        "      self.conv_res1_64 = torch.nn.Conv2d(in_channels, out_channels, kernel_size=3, stride=stride, padding=1, bias=True)\n",
        "      self.bn1_64 = torch.nn.BatchNorm2d(out_channels)\n",
        "      self.conv_res2_64 = torch.nn.Conv2d(out_channels, out_channels, kernel_size=3, stride=1, padding=1, bias=True)\n",
        "      self.bn2_64 = torch.nn.BatchNorm2d(out_channels)\n",
        "\n",
        "      self.conv_res1_32 = torch.nn.Conv2d(in_channels, out_channels, kernel_size=3, stride=stride, padding=1, bias=True)\n",
        "      self.bn1_32 = torch.nn.BatchNorm2d(out_channels)\n",
        "      self.conv_res2_32 = torch.nn.Conv2d(out_channels, out_channels, kernel_size=3, stride=1, padding=1, bias=True)\n",
        "      self.bn2_32 = torch.nn.BatchNorm2d(out_channels)\n",
        "                                                                                            # We use one Resnet block per resolution.\n",
        "\n",
        "      self.pool =  torch.nn.MaxPool2d(kernel_size=2, stride = 2) #divides by 2 the x length, y length ie downsamples, no weights we can reuse it\n",
        "\n",
        "      self.conv_expansion = torch.nn.Conv2d(in_channels=64, out_channels=128, kernel_size=1, stride=1, padding=0)\n",
        "      self.conv_expansion_input = torch.nn.Conv2d(in_channels=input_channel, out_channels=64, kernel_size=1, stride=1, padding=0)\n",
        "\n",
        "      #add a layer to get from\n",
        "\n",
        "\n",
        "    def forward_up_to_64_layer(self,x):\n",
        "      x = self.conv_expansion_input(x)\n",
        "      #256 pixels\n",
        "      out = self.relu(self.bn1_256(self.conv_res1_256(x)))\n",
        "      out = self.bn2_256(self.conv_res2_256(out))\n",
        "      out += x\n",
        "      out256 = self.relu(out)\n",
        "      out256 = self.pool(out256)\n",
        "      #128 pixels\n",
        "      out128 = self.relu(self.bn1_128(self.conv_res1_128(out256)))\n",
        "      out128 = self.bn2_128(self.conv_res2_128(out128))\n",
        "      out128 += out256\n",
        "      out128 = self.relu(out128)\n",
        "\n",
        "      out128 = self.pool(out128) #change pixel size\n",
        "      out128 = self.conv_expansion(out128) #change nb of channels\n",
        "      #64 pixels\n",
        "      out64 = self.relu(self.bn1_64(self.conv_res1_64(out128)))\n",
        "      out64 = self.bn2_64(self.conv_res2_64(out64))\n",
        "      out64 += out128\n",
        "      out64 = self.relu(out64)\n",
        "      return out64\n",
        "\n",
        "    def forward(self, x):\n",
        "      x = self.conv_expansion_input(x)\n",
        "      #256 pixels\n",
        "      out = self.relu(self.bn1_256(self.conv_res1_256(x)))\n",
        "      out = self.bn2_256(self.conv_res2_256(out))\n",
        "      out += x\n",
        "      out256 = self.relu(out)\n",
        "      out256 = self.pool(out256)\n",
        "      #128 pixels\n",
        "      out128 = self.relu(self.bn1_128(self.conv_res1_128(out256)))\n",
        "      out128 = self.bn2_128(self.conv_res2_128(out128))\n",
        "      out128 += out256\n",
        "      out128 = self.relu(out128)\n",
        "\n",
        "      out128 = self.pool(out128) #change pixel size\n",
        "      out128 = self.conv_expansion(out128) #change nb of channels\n",
        "      #64 pixels\n",
        "      out64 = self.relu(self.bn1_64(self.conv_res1_64(out128)))\n",
        "      out64 = self.bn2_64(self.conv_res2_64(out64))\n",
        "      out64 += out128\n",
        "      out64 = self.relu(out64)\n",
        "      out64 = self.pool(out64) #change pixel size\n",
        "\n",
        "      #32 pixels\n",
        "      out32 = self.relu(self.bn1_32(self.conv_res1_32(out64)))\n",
        "      out32 = self.bn2_32(self.conv_res2_32(out32))\n",
        "      out32 += out64\n",
        "      out32 = self.relu(out32)\n",
        "\n",
        "      return out32\n"
      ]
    },
