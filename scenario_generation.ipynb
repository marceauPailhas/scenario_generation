        "            )\n",
        "\n",
        "            sizes = standard_sizes[track.object_type]\n",
        "\n",
        "            box_parameters = np.array([sizes[1]/2, sizes[0]/2]*2)\n",
        "            current_position = actor_trajectory[-1,:]\n",
        "            current_heading = actor_headings[-1]\n",
        "            box_parameters = np.concatenate(\n",
        "                (\n",
        "                        [np.cos(current_heading),np.sin(current_heading)], np.log(box_parameters)\n",
        "                        ),\n",
        "                                            axis = 0\n",
        "                                            )\n",
        "\n",
        "            traj_diff = np.diff(actor_trajectory, axis =0)\n",
        "            future_traj_diff = np.diff(np.concatenate( (current_position.reshape(-1,2),future_trajectory.reshape(-1,2)) , axis =0), axis = 0)\n",
        "            left_padded_traj, right_padded_future_traj = pad_vectors(traj_diff, future_traj_diff, 2)\n",
        "            trajectory = np.concatenate((left_padded_traj, right_padded_future_traj), axis=0)\n",
        "\n",
        "\n",
        "            headings_diff = np.diff(actor_headings, axis = 0)\n",
        "            future_headings_diff = np.diff(np.concatenate( ([current_heading],future_headings.reshape(-1)) , axis =0), axis = 0)\n",
        "            left_padded_headings, right_padded_future_headdings = \\\n",
        "                                    pad_vectors(headings_diff.reshape(-1,1), future_headings_diff.reshape(-1,1), 2)\n",
        "            headings = np.concatenate((left_padded_headings, right_padded_future_headdings), axis=0)\n",
        "\n",
        "\n",
        "            ## Concatenate trajectory and headings ##\n",
        "            trajectory_and_heading = np.concatenate((trajectory,headings), axis = 1).flatten()\n",
        "            recentered_current_position = (current_position-np.array(track_bounds))\n",
        "            label = np.concatenate(( recentered_current_position , box_parameters,trajectory_and_heading), axis = 0)\n",
        "\n",
        "            labels = np.concatenate((labels, label.reshape(1,-1)), axis = 0)\n",
        "    return labels\n",
        "    if 40-labels.shape[0] < 0 :\n",
        "      return labels[:40,:]\n",
        "    return np.pad(labels, ((0, 40-labels.shape[0]), (0, 0)), mode='constant', constant_values=0)\n",
        "\n",
