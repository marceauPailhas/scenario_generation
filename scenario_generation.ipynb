        "def get_labels(scenario, timestep ):\n",
        "\n",
        "    before_timestep = [timestep -20, timestep -10 , timestep]\n",
        "    after_timestep = [timestep +10, timestep +20]\n",
        "    labels = np.zeros((0,20))\n",
        "\n",
        "    for track in scenario.tracks:\n",
        "        if track.category == TrackCategory.FOCAL_TRACK:\n",
        "            position: NDArrayFloat = np.array([list(object_state.position) for object_state in track.object_states\n",
        "                                  if object_state.timestep == timestep ])\n",
        "            if position.shape[0] < 1:\n",
        "                raise ValueError(f\"Focal track not found at timestep {timestep}\")\n",
        "            x_center = position[0,0]\n",
        "            y_center = position[0,1]\n",
        "            track_bounds = (x_center, y_center)\n",
        "            break\n",
        "\n",
        "    for track in scenario.tracks:\n",
        "            if track.object_type in _STATIC_OBJECT_TYPES.union({ObjectType.PEDESTRIAN}):\n",
        "                continue\n",
        "            if not(track.object_type in standard_sizes.keys()):\n",
        "                continue\n",
        "            # Get timesteps for which actor data is valid\n",
        "            actor_timesteps: NDArrayInt = np.array(\n",
        "                [object_state.timestep for object_state in track.object_states if object_state.timestep in before_timestep]\n",
        "            )\n",
        "            if actor_timesteps.shape[0] < 1 or actor_timesteps[-1] != timestep:\n",
        "                continue\n",
        "\n",
        "\n",
        "            # Get actor trajectory and heading history\n",
        "            actor_trajectory: NDArrayFloat = np.array(\n",
        "                [list(object_state.position) for object_state in track.object_states\n",
        "                                      if object_state.timestep in before_timestep ]\n",
        "            )\n",
        "            if max ( abs(actor_trajectory[-1,0]-x_center), abs(actor_trajectory[-1,1] - y_center) ) \\\n",
        "                              > _PLOT_BOUNDS_BUFFER_M :\n",
        "                continue\n",
        "\n",
        "            future_trajectory : NDArrayFloat = np.array(\n",
        "                [list(object_state.position) for object_state in track.object_states\n",
        "                                      if object_state.timestep in after_timestep ]\n",
        "            )\n",
        "\n",
        "            actor_headings: NDArrayFloat = np.array(\n",
        "                [object_state.heading for object_state in track.object_states\n",
        "                                      if object_state.timestep in before_timestep ]\n",
