        "              attention_mask = attention_mask.type(torch.bool).to(device)\n",
        "\n",
        "              # Forward pass\n",
        "              z = Epsilon(x)\n",
        "              combined_input = torch.cat((Ddown(m), z), dim=1)\n",
        "              sparse = Dup(combined_input)\n",
        "\n",
        "\n",
        "              #Compute proxies for q(z), and standard normal\n",
        "              # interval_prob = torch.log ( get_interval_probabilities(z) )\n",
        "              # z_prob = torch.log ( torch.ones(interval_prob.shape)/interval_prob.shape[1] )\n",
