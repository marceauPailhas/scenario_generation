      "source": [
        "def train_diffusion(Epsilon, Epsilon_m, M , key_MLP, value_MLP, train_loader, val_loader, num_epochs, criterion, optimizer,\n",
        "                    selected_token_params, device, P_mean =  -0.5 , P_sigma = 1, sigma_data = 0.5):\n",
        "    scheduler = torch.optim.lr_scheduler.ReduceLROnPlateau(\n",
        "        optimizer, factor=0.1, patience=5, verbose=True\n",
        "    )\n",
        "\n",
        "    def get_concatenated_array_with_mask(targets, selected_params=selected_token_params):\n",
        "        \"\"\"\n",
        "        Given the targets tensor and a list of selected parameter names,\n",
        "        returns the concatenated array corresponding to the chosen parameters using a mask.\n",
        "        \"\"\"\n",
        "        B, N, D = targets.shape\n",
        "\n",
        "        # Define the mask map for each parameter\n",
        "        param_mask = {\n",
        "            'box_current_position': torch.arange(D) < 2,\n",
        "            'heading': (torch.arange(D) >= 2) & (torch.arange(D) < 4),\n",
        "            'box_parameters': (torch.arange(D) >= 4) & (torch.arange(D) < 6),\n",
        "            'current_speed': (torch.arange(D) % 3 != 1) & (torch.arange(D) >= 11) & (torch.arange(D) < 17),\n",
        "            'final_speed': (torch.arange(D) % 3 != 1) & (torch.arange(D) >= 17),\n",
        "            'relative_heading_from_0s_2s': (torch.arange(D) % 3 == 1) & (torch.arange(D) >= 14),\n",
        "        }\n",
        "\n",
        "        selected_tensors = []\n",
        "        for param in selected_params:\n",
        "            if param == 'heading':\n",
        "                cos_vals = targets[..., 2]\n",
        "                sin_vals = targets[..., 3]\n",
        "                heading = torch.atan2(sin_vals, cos_vals).unsqueeze(-1)\n",
        "                selected_tensors.append(heading)\n",
        "            elif param == 'box_parameters':\n",
        "                box_params = targets[..., 4:6]\n",
        "                transformed_box_params = 2 * torch.exp(box_params)\n",
        "                selected_tensors.append(transformed_box_params)\n",
        "            elif param == 'relative_heading_from_0s_2s':\n",
        "                relative_heading = targets[..., param_mask[param]].sum(dim=-1, keepdim=True)\n",
        "                selected_tensors.append(relative_heading)\n",
        "            elif param == 'current_speed':\n",
        "                relevant_data = targets[..., param_mask[param]]  # Shape: (..., 4) assuming vx(-1), vy(-1), vx(1), vy(1)\n",
        "\n",
        "                # Compute the means of vx and vy separately\n",
        "                vx_mean = relevant_data[..., ::2].mean(dim=-1, keepdim=True)  # vx(-1) and vx(1)\n",
        "                vy_mean = relevant_data[..., 1::2].mean(dim=-1, keepdim=True)  # vy(-1) and vy(1)\n",
        "\n",
        "                # Combine the means into a single tensor\n",
        "                current_speed = torch.cat((vx_mean, vy_mean), dim=-1)\n",
        "                selected_tensors.append(current_speed)\n",
        "            else:\n",
        "                selected_tensors.append(targets[..., param_mask[param]])\n",
        "\n",
        "        result = torch.cat(selected_tensors, dim=-1)\n",
        "        return result\n",
        "\n",
        "    def sample_pmask():\n",
        "        if torch.rand(1).item() < 0.4:\n",
        "            return 0.0\n",
        "        else:\n",
        "            return torch.distributions.Beta(2, 1).sample().item()\n",
        "\n",
        "    def bucket_pmask(pmask):\n",
        "        if pmask < 0 or pmask > 1:\n",
        "            raise ValueError(\"pmask should be between 0 and 1\")\n",
        "        bins = torch.linspace(0, 1, steps=11)\n",
        "        bucket = torch.bucketize(torch.tensor([pmask]), bins)\n",
        "        # Clamp the bucket index to be within the valid range for one_hot\n",
        "        bucket = torch.clamp(bucket, 0, 9)\n",
        "        one_hot = F.one_hot(bucket, num_classes=10).float().squeeze(0)\n",
        "\n",
        "        return one_hot\n",
        "\n",
        "    for epoch in range(num_epochs):\n",
        "        total_loss = 0\n",
        "        for batch in train_loader:\n",
        "            x,m, x_label, attention_mask = batch['x'], batch['m'], batch[\"x_label\"], batch[\"attention_mask\"]\n",
        "            x = x.type(torch.float).to(device)\n",
        "            m = m.type(torch.float).to(device)\n",
        "            x_label = x_label.type(torch.float).to(device)\n",
        "            attention_mask = attention_mask.type(torch.bool).to(device)\n",
        "\n",
        "\n",
        "            # Do not exclude targets where x_label is all zeros; work with the full batch\n",
        "            #targets = x_label  # Shape: (B, 40, 20)\n",
        "            tokens = get_concatenated_array_with_mask(x_label, selected_params=selected_token_params)\n",
        "\n",
        "\n",
        "            optimizer.zero_grad()\n",
        "\n",
        "            # Sample a unique pmask for each sample in the batch\n",
        "            pmasks = torch.tensor([sample_pmask() for _ in range(tokens.size(0))], device=device)  # Shape: (B,)\n",
        "\n",
        "            # Bucket each pmask\n",
        "            global_scene_token = torch.stack([bucket_pmask(pmask) for pmask in pmasks], dim=0).unsqueeze(1).to(device)  # Shape: (B, num_classes)\n",
        "\n",
        "            # Masking rows\n",
        "            row_masks = torch.rand(tokens.size(0), tokens.size(1), device=device) < pmasks[:, None]  # Shape: (B, N)\n",
        "            agent_tokens = tokens.clone()\n",
        "            agent_tokens[row_masks,:] = 0  # Zero-out selected rows for each batch element\n",
        "\n",
        "\n",
        "\n",
        "            # Forward pass\n",
        "            z_hat = Epsilon(x)\n",
        "            key = key_MLP(agent_tokens, attention_mask) #attention mask enables to know which row corresponds to a present vehicle\n",
        "            value = value_MLP(global_scene_token)\n",
        "            m_latent = Epsilon_m(m)\n",
        "\n",
        "            # Add noise\n",
        "            eps = torch.normal(0, 1, size=z_hat.size(), device=device)\n",
        "            sigma = torch.exp(torch.normal(P_mean, P_sigma, size=(), device=device)).item() #hee\n",
        "            z = z_hat + sigma * eps\n",
        "\n",
        "            c_in = 1 / np.sqrt(sigma**2 + sigma_data**2)\n",
        "            predicted_latent = M(torch.cat([c_in * z, m_latent], dim=1), key, value)\n",
        "            loss = criterion(z, z_hat, predicted_latent, sigma)\n",
        "\n",
        "            # Backward pass\n",
        "            loss.backward()\n",
        "            optimizer.step()\n",
        "            total_loss += loss.item()\n",
        "\n",
        "        # Validation\n",
        "        with torch.no_grad():\n",
        "            val_loss = 0\n",
        "            for batch in val_loader:\n",
        "                x,m, x_label, attention_mask = batch['x'], batch['m'], batch[\"x_label\"], batch[\"attention_mask\"]\n",
        "                x = x.type(torch.float).to(device)\n",
        "                m = m.type(torch.float).to(device)\n",
        "                x_label = x_label.type(torch.float).to(device)\n",
        "                attention_mask = attention_mask.type(torch.bool).to(device)\n",
        "\n",
        "                #targets = x_label\n",
        "                tokens = get_concatenated_array_with_mask(x_label, selected_params=selected_token_params)\n",
        "\n",
        "                # Sample a unique pmask for each sample in the batch\n",
        "                pmasks = torch.tensor([sample_pmask() for _ in range(tokens.size(0))], device=device)  # Shape: (B,)\n",
        "\n",
        "                # Bucket each pmask\n",
        "                global_scene_token = torch.stack([bucket_pmask(pmask) for pmask in pmasks], dim=0).unsqueeze(1).to(device)  # Shape: (B, num_classes)\n",
        "\n",
        "                # Masking rows\n",
        "                row_masks = torch.rand(tokens.size(0), tokens.size(1), device=device) < pmasks[:, None]  # Shape: (B, N)\n",
        "                agent_tokens = tokens.clone()\n",
        "                agent_tokens[row_masks,:] = 0  # Zero-out selected rows for each batch element\n",
        "\n",
        "\n",
        "                # Forward pass\n",
        "                z_hat = Epsilon(x)\n",
        "                key = key_MLP(agent_tokens, attention_mask)\n",
        "                value = value_MLP(global_scene_token)\n",
        "                m_latent = Epsilon_m(m)\n",
        "\n",
        "                # Add noise\n",
        "                eps = torch.normal(0, 1, size=z_hat.size(), device=device)\n",
        "                sigma = torch.exp(torch.normal(P_mean, P_sigma, size=(), device=device)).item()\n",
        "                z = z_hat + sigma * eps\n",
        "\n",
        "                c_in = 1 / np.sqrt(sigma**2 + sigma_data**2)\n",
        "                predicted_latent = M(torch.cat([c_in * z, m_latent], dim=1), key, value)\n",
        "                val_loss += criterion(z, z_hat, predicted_latent, sigma)\n",
        "\n",
        "            val_loss /= len(val_loader)\n",
        "            scheduler.step(val_loss)\n",
        "\n",
        "        print(f\"Epoch {epoch + 1}/{num_epochs}, Loss: {total_loss / len(train_loader):.4f}, Val Loss: {val_loss:.4f}\")\n",
        "\n",
        "        # Save models\n",
        "        # Define model paths in Google Drive\n",
        "        model_save_dir = 'models'  # Change this to your preferred location\n",
        "        os.makedirs(model_save_dir, exist_ok=True)\n",
        "\n",
        "        # Save paths for each model\n",
        "        unet_path = f'{model_save_dir}/UNet_{epoch}.pth'\n",
        "        key_mlp_path = f'{model_save_dir}/Key_MLP_{epoch}.pth'\n",
        "        value_mlp_path = f'{model_save_dir}/Value_MLP_{epoch}.pth'\n",
        "\n",
        "        # Save the model weights\n",
        "        torch.save(M.state_dict(), unet_path)\n",
        "        torch.save(key_MLP.state_dict(), key_mlp_path)\n",
        "        torch.save(value_MLP.state_dict(), value_mlp_path)\n",
        "        print(f\"Model saved at epoch {epoch}\")\n",
